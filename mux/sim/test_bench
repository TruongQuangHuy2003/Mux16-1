#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5d450db1edd0 .scope module, "test_bench" "test_bench" 2 2;
 .timescale -9 -12;
v0x5d450db42830_0 .var "data_in", 15 0;
v0x5d450db42910_0 .var "en", 0 0;
v0x5d450db429e0_0 .var/i "i", 31 0;
v0x5d450db42ab0_0 .var "sel", 3 0;
v0x5d450db42b80_0 .net "y", 0 0, v0x5d450db426c0_0;  1 drivers
S_0x5d450db1ef60 .scope function.vec4.s1, "calc_expected" "calc_expected" 2 57, 2 57 0, S_0x5d450db1edd0;
 .timescale -9 -12;
; Variable calc_expected is vec4 return value of scope S_0x5d450db1ef60
v0x5d450db41c20_0 .var "en_val", 0 0;
v0x5d450db41ce0_0 .var "in", 15 0;
v0x5d450db41da0_0 .var "sel_val", 3 0;
TD_test_bench.calc_expected ;
    %load/vec4 v0x5d450db41c20_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0 T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %jmp/1 T_0.1, 8;
T_0.0 ; End of true expr.
    %load/vec4 v0x5d450db41ce0_0;
    %load/vec4 v0x5d450db41da0_0;
    %part/u 1;
    %jmp/0 T_0.1, 8;
 ; End of false expr.
    %blend;
T_0.1;
    %ret/vec4 0, 0, 1;  Assign to calc_expected (store_vec4_to_lval)
    %end;
S_0x5d450db41e80 .scope task, "check_result" "check_result" 2 64, 2 64 0, S_0x5d450db1edd0;
 .timescale -9 -12;
v0x5d450db42080_0 .var "expected_result", 0 0;
TD_test_bench.check_result ;
    %vpi_call 2 67 "$display", "At time: %t, en = 1'b%b, sel = 4'b%b, data_in = 16'b%b", $time, v0x5d450db42910_0, v0x5d450db42ab0_0, v0x5d450db42830_0 {0 0 0};
    %load/vec4 v0x5d450db42b80_0;
    %load/vec4 v0x5d450db42080_0;
    %cmp/e;
    %jmp/0xz  T_1.2, 4;
    %vpi_call 2 69 "$display", "-------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 70 "$display", "PASSED: expected: 1'b%b, Got: 1'b%b", v0x5d450db42080_0, v0x5d450db42b80_0 {0 0 0};
    %vpi_call 2 71 "$display", "-------------------------------------------------------------------------------" {0 0 0};
    %jmp T_1.3;
T_1.2 ;
    %vpi_call 2 73 "$display", "-------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 74 "$display", "FAILED: expected: 1'b%b, Got: 1'b%b", v0x5d450db42080_0, v0x5d450db42b80_0 {0 0 0};
    %vpi_call 2 75 "$display", "-------------------------------------------------------------------------------" {0 0 0};
T_1.3 ;
    %end;
S_0x5d450db42140 .scope module, "uut" "mux16to1" 2 8, 3 1 0, S_0x5d450db1edd0;
 .timescale 0 0;
    .port_info 0 /INPUT 16 "data_in";
    .port_info 1 /INPUT 4 "sel";
    .port_info 2 /INPUT 1 "en";
    .port_info 3 /OUTPUT 1 "y";
v0x5d450db42410_0 .net "data_in", 15 0, v0x5d450db42830_0;  1 drivers
v0x5d450db42510_0 .net "en", 0 0, v0x5d450db42910_0;  1 drivers
v0x5d450db425d0_0 .net "sel", 3 0, v0x5d450db42ab0_0;  1 drivers
v0x5d450db426c0_0 .var "y", 0 0;
E_0x5d450dae5a70 .event anyedge, v0x5d450db42510_0, v0x5d450db425d0_0, v0x5d450db42410_0;
    .scope S_0x5d450db42140;
T_2 ;
    %wait E_0x5d450dae5a70;
    %load/vec4 v0x5d450db42510_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0 T_2.0, 8;
    %pushi/vec4 0, 0, 1;
    %jmp/1 T_2.1, 8;
T_2.0 ; End of true expr.
    %load/vec4 v0x5d450db42410_0;
    %load/vec4 v0x5d450db425d0_0;
    %part/u 1;
    %jmp/0 T_2.1, 8;
 ; End of false expr.
    %blend;
T_2.1;
    %store/vec4 v0x5d450db426c0_0, 0, 1;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x5d450db1edd0;
T_3 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5d450db429e0_0, 0, 32;
    %end;
    .thread T_3;
    .scope S_0x5d450db1edd0;
T_4 ;
    %vpi_call 2 18 "$dumpfile", "test_bench.vcd" {0 0 0};
    %vpi_call 2 19 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5d450db1edd0 {0 0 0};
    %vpi_call 2 21 "$display", "-------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 22 "$display", "------------------------TESTBENCH FOR MULTIPLEXER 16:1-------------------------" {0 0 0};
    %vpi_call 2 23 "$display", "-------------------------------------------------------------------------------" {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5d450db429e0_0, 0, 32;
T_4.0 ;
    %load/vec4 v0x5d450db429e0_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_4.1, 5;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d450db42910_0, 0, 1;
    %vpi_func 2 27 "$random" 32 {0 0 0};
    %pushi/vec4 65535, 0, 32;
    %mod/s;
    %pad/s 16;
    %store/vec4 v0x5d450db42830_0, 0, 16;
    %load/vec4 v0x5d450db429e0_0;
    %pad/s 4;
    %store/vec4 v0x5d450db42ab0_0, 0, 4;
    %delay 1000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d450db42080_0, 0, 1;
    %fork TD_test_bench.check_result, S_0x5d450db41e80;
    %join;
    %delay 10000, 0;
    %load/vec4 v0x5d450db429e0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5d450db429e0_0, 0, 32;
    %jmp T_4.0;
T_4.1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5d450db429e0_0, 0, 32;
T_4.2 ;
    %load/vec4 v0x5d450db429e0_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_4.3, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d450db42910_0, 0, 1;
    %vpi_func 2 36 "$random" 32 {0 0 0};
    %pushi/vec4 65535, 0, 32;
    %mod/s;
    %pad/s 16;
    %store/vec4 v0x5d450db42830_0, 0, 16;
    %load/vec4 v0x5d450db429e0_0;
    %pad/s 4;
    %store/vec4 v0x5d450db42ab0_0, 0, 4;
    %delay 1000, 0;
    %load/vec4 v0x5d450db42830_0;
    %load/vec4 v0x5d450db42ab0_0;
    %load/vec4 v0x5d450db42910_0;
    %store/vec4 v0x5d450db41c20_0, 0, 1;
    %store/vec4 v0x5d450db41da0_0, 0, 4;
    %store/vec4 v0x5d450db41ce0_0, 0, 16;
    %callf/vec4 TD_test_bench.calc_expected, S_0x5d450db1ef60;
    %store/vec4 v0x5d450db42080_0, 0, 1;
    %fork TD_test_bench.check_result, S_0x5d450db41e80;
    %join;
    %delay 10000, 0;
    %load/vec4 v0x5d450db429e0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5d450db429e0_0, 0, 32;
    %jmp T_4.2;
T_4.3 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5d450db429e0_0, 0, 32;
T_4.4 ;
    %load/vec4 v0x5d450db429e0_0;
    %cmpi/s 20, 0, 32;
    %jmp/0xz T_4.5, 5;
    %vpi_func 2 44 "$random" 32 {0 0 0};
    %pushi/vec4 2, 0, 32;
    %mod/s;
    %pad/s 1;
    %store/vec4 v0x5d450db42910_0, 0, 1;
    %vpi_func 2 45 "$random" 32 {0 0 0};
    %pushi/vec4 65535, 0, 32;
    %mod/s;
    %pad/s 16;
    %store/vec4 v0x5d450db42830_0, 0, 16;
    %vpi_func 2 46 "$random" 32 {0 0 0};
    %pushi/vec4 16, 0, 32;
    %mod/s;
    %pad/s 4;
    %store/vec4 v0x5d450db42ab0_0, 0, 4;
    %delay 1000, 0;
    %load/vec4 v0x5d450db42830_0;
    %load/vec4 v0x5d450db42ab0_0;
    %load/vec4 v0x5d450db42910_0;
    %store/vec4 v0x5d450db41c20_0, 0, 1;
    %store/vec4 v0x5d450db41da0_0, 0, 4;
    %store/vec4 v0x5d450db41ce0_0, 0, 16;
    %callf/vec4 TD_test_bench.calc_expected, S_0x5d450db1ef60;
    %store/vec4 v0x5d450db42080_0, 0, 1;
    %fork TD_test_bench.check_result, S_0x5d450db41e80;
    %join;
    %delay 10000, 0;
    %load/vec4 v0x5d450db429e0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5d450db429e0_0, 0, 32;
    %jmp T_4.4;
T_4.5 ;
    %delay 100000, 0;
    %vpi_call 2 53 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "../tb/test_bench.v";
